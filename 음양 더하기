## 음양 더하기
## 문제 설명

## 문제 url: https://programmers.co.kr/learn/courses/30/lessons/76501

function solution(absolutes, signs) {
    let sum = 0
    
    for(let i = 0; i < absolutes.length; i++) {
        if(signs[i] === true) sum = sum + absolutes[i]
        else sum = sum - absolutes[i]
    }
    
    return sum
}
